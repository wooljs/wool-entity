  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nicocube/wool-entity/issues"
  },
  "homepage": "https://github.com/nicocube/wool-entity#readme",
  "dependencies": {
    "wool-validate": "^2.5.1"
  },
  "devDependencies": {
    "@freecube/tap-summary": "^4.0.4",
    "c8": "^10.1.3",
    "email-address": "^1.2.2",
    "nodemon": "^3.1.10",
    "standard": "^17.1.2",
    "tape": "^5.9.0",
    "wool-store": "^2.5.0"
  }
